require(dplyr); require(stringr)
let <- c(sample(letters), sample(letters) %>% toupper, # character
sample(9) %>% rep(3) )                        # number
dat <- sample(let)[1:8]
n <- length(dat)
prop <- rep(1/n, 8)
pass <- sample(dat, n, replace=FALSE, prop) %>% str_c(collapse = "")
print(pass)
